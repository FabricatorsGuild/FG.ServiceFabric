{
	"Loggers": [
		{
			"Name": "IServiceCommunicationAndClientLogger",
			"StartId": 1000,
			"ImplicitArguments": [
				{
					"Name": "context",
					"Type": "System.Fabric.ServiceContext",
					"Assignment": "($this).ToString()"
				}
			]
		}
	],
	"Name": "FGServiceFabricServicesRemotingEventSource",
	"ProviderName": "FG-ServiceFabric",
	"Keywords": [
		"ServiceClient",
		"ServiceCommunication",
		"ServiceRemoting"
	],
	"TypeTemplates": [
		{
			"Name": "Exception",
			"CLRType": "System.Exception",
			"Arguments": [
				{
					"Name": "message",
					"Type": "string",
					"Assignment": "$this.Message"
				},
				{
					"Name": "source",
					"Type": "string",
					"Assignment": "$this.Source"
				},
				{
					"Name": "exceptionTypeName",
					"Type": "string",
					"Assignment": "$this.GetType().FullName"
				},
				{
					"Name": "stackTrace",
					"Type": "string",
					"Assignment": "$this.StackTrace"
				}
			]
		},
		{
			"Name": "StatefulServiceContext",
			"CLRType": "System.Fabric.StatefulServiceContext",
			"Arguments": [
				{
					"Assignment": "$this.ServiceName.ToString()",
					"Name": "serviceName",
					"Type": "string",
					"CLRType": "string"
				},
				{
					"Assignment": "$this.ServiceTypeName",
					"Name": "serviceTypeName",
					"Type": "string",
					"CLRType": "string"
				},
				{
					"Assignment": "$this.ReplicaOrInstanceId",
					"Name": "replicaOrInstanceId",
					"Type": "long",
					"CLRType": "long"
				},
				{
					"Assignment": "$this.PartitionId",
					"Name": "partitionId",
					"Type": "Guid",
					"CLRType": "System.Guid"
				},
				{
					"Assignment": "$this.CodePackageActivationContext.ApplicationName",
					"Name": "applicationName",
					"Type": "string",
					"CLRType": "string"
				},
				{
					"Assignment": "$this.CodePackageActivationContext.ApplicationTypeName",
					"Name": "applicationTypeName",
					"Type": "string",
					"CLRType": "string"
				},
				{
					"Assignment": "$this.NodeContext.NodeName",
					"Name": "nodeName",
					"Type": "string",
					"CLRType": "string"
				}
			]
		}
	],
	"Settings": {
		"Modules": [],
		"AutogenerateLoggerInterfaces": false,
		"ImplicitTypeTemplates": false,
		"UseReferencedHelpers": false
	}
}